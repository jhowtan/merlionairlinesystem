<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="PersistenceUnit">
        <jta-data-source>jdbc/MAS</jta-data-source>
        <class>MAS.Entity.User</class>
        <class>MAS.Entity.Role</class>
        <class>MAS.Entity.Permission</class>
        <class>MAS.Entity.AuditLog</class>
        <class>MAS.Entity.AircraftType</class>
        <class>MAS.Entity.AircraftSeatConfig</class>
        <class>MAS.Entity.Aircraft</class>
        <class>MAS.Entity.Airport</class>
        <class>MAS.Entity.Country</class>
        <class>MAS.Entity.City</class>
        <class>MAS.Entity.AircraftAssignment</class>
        <class>MAS.Entity.Route</class>
        <class>MAS.Entity.Workgroup</class>
        <class>MAS.Entity.Message</class>
        <class>MAS.Entity.Flight</class>
        <class>MAS.Entity.FareRule</class>
        <class>MAS.Entity.AircraftMaintenanceSlot</class>
        <class>MAS.Entity.BookingClass</class>
        <class>MAS.Entity.FlightGroup</class>
        <class>MAS.Entity.Cost</class>
        <class>MAS.Entity.Customer</class>
        <class>MAS.Entity.StringAttribute</class>
        <class>MAS.Entity.DoubleAttribute</class>
        <class>MAS.Entity.BooleanAttribute</class>
        <class>MAS.Entity.IntAttribute</class>
        <class>MAS.Entity.Baggage</class>
        <class>MAS.Entity.PNR</class>
        <class>MAS.Entity.Itinerary</class>
        <class>MAS.Entity.SpecialServiceRequest</class>
        <class>MAS.Entity.ETicket</class>
        <class>MAS.Entity.Certification</class>
        <shared-cache-mode>NONE</shared-cache-mode>
        <properties>
            <property name="javax.persistence.schema-generation.database.action" value="create" />
        </properties>
    </persistence-unit>
</persistence>
